<%= params.inspect %>
<%= render "movieinfo" %>
<table class="table col-md-12">
  <tbody>
    <% @reviews.each do |review| %>
    <tr>
      <td>
        <%= review.user.email %> 看过 <%= review.updated_at %> <br/>
        <%= review.content %>
      </td>
    </tr>
    <% end %>

  </tbody>
</table>
